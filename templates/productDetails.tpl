<table border="0" cellpadding="0" cellspacing="0" class="productDetails">
	<tr class="prodId">
		<th>Product</th>
		<td>{Product_ID}</td>
	</tr>
	<tr class="Seperator">
		<td colspan="2">&nbsp</td>
	</tr>
	<tr>
		<th>Title</th>
		<td>{Model_Name}</td>
	</tr>
	<tr>
		<th>Image</th>
		<td>
			<span class="lineData">{HighPic}</span>
			<img src="{HighPic}" alt="{HighPic}" title="{HighPic}" />
		</td>
	</tr>
	<tr>
		<th>Detail-XML</th>
		<td>{path}</td>
	</tr>
	<tr>
		<th>EANs</th>
		<td>{eanTable}</td>
	</tr>
</table>
<div class="seperatorDiv"></div>